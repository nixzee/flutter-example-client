{
    "version": "2.0.0",
    "tasks":[
        {
            "label": "Flutter Build Runner",
            "type": "shell",
            "command": "flutter pub run build_runner build --delete-conflicting-outputs",
            "dependsOn":[
                "Flutter Packages - Common"
            ],
            "runOptions": {
                "runOn": "folderOpen"
            },
            "problemMatcher": [
                "$dart-build_runner"
            ],
            "isBackground": true,
            "options": {
                "cwd": "${workspaceFolder}/flutter_example_client"
            }
        }
    ]
}
